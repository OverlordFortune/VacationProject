<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="icon"
          href="~/Content/Icons/logo-softheme.svg">
    <title>SofthemeVacations</title>
    @Styles.Render("~/Content/Styles/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body id="fontBody" class="p-0">
    <div class="col-12">

        <div class="row  bg-dark">
            <div class="col-ms-0 col-lg-2 col-md-2 m-0 p-0"></div>
            <div class="col-12 col-md-8 col-lg-8 m-0 p-0">
                @Html.Partial("_NavbarTopPartial")
            </div>
        </div>
        <div class="row row-second">
            <div class="col-2 m-0 p-0">
                @Html.Partial("_LeftMenuPartial")
            </div>
            <div class="col-8 p-0">
                <div class="container">
                    @RenderBody()
                </div>
            </div>
        </div>
    </div>


    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)

</body>
</html>
